<div id='project'>
  <h1>{{project.project_title}}</h1>
  <h3>{{project.start_date}}</h3>
  <h5>Tasks:</h5>
  <ul>
    {{#each tasks}}
      <li><a href="/#/tasks/{{id}}">{{title}}</a></li>
    {{/each}}
  </ul>
  <button id="new-task-button" type="">New Task</button>
  {{> taskform-partial}}
</div>
